{
  "name": "attendance-backend",
  "version": "1.0.0",
  "description": "Backend API for Mobile Attendance System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "npx --yes prisma@5.7.1 generate",
    "prisma:migrate": "npx --yes prisma@5.7.1 migrate dev --skip-generate",
    "prisma:studio": "npx --yes prisma@5.7.1 studio",
    "lint": "eslint src/**/*.ts",
    "test": "npx jest"
  },
  "keywords": [
    "attendance",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "abbrev": "4.0.0",
    "are-we-there-yet": "4.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "8.1.0",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "socket.io": "^4.5.4",
    "winston": "3.19.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "0.0.48",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "eslint": "9.39.1",
    "jest": "30.2.0",
    "prisma": "^5.7.1",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsx": "^4.7.0",
    "typescript": "5.9.3"
  }
